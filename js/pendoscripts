<script>
function getLinkWithQueryParam(name){
  // Get the button element
  let rawPage = document.getElementById(name).innerHTML;
  let page = rawPage.toLowerCase()
	let mainUrl = 'https://omerwexler86.github.io/acme/'
  // Get the current query string
  let queryString = window.location.search;
	
  // Set the button's href attribute to the desired URL, appending the current query string
  window.location.href = mainUrl+ page + '.html' + queryString
  console.log(mainUrl+ page + '.html' + queryString )
  }
</script>
	    <script>
function getQueryFromUrl2(){

let mainUrl = 'https://omerwexler86.github.io/acme/index'
  // Get the current query string
  let queryString = window.location.search;
 window.location.href = mainUrl + '.html' + queryString
}

</script>


